FoamFile
{
    version         2.0;
    format          ascii;

    root            "";
    case            "";
    instance        "";
    local           "";

    class           dictionary;
    object          blockMeshDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

vertices
(
//Plane A:
(0.0 0.0173241161390262 -0.0173241161391146 ) // Vertex A0 = 0 
(0.0 0.0173241161390262 0.0173241161391146 ) // Vertex A1 = 1 
(0.0 -0.0173241161390262 0.0173241161391146 ) // Vertex A2 = 2 
(0.0 -0.0173241161390262 -0.0173241161391146 ) // Vertex A3 = 3 
(0.0 0.024748737341466 -0.0247487373415923 ) // Vertex A4 = 4 
(0.0 0.024748737341466 0.0247487373415923 ) // Vertex A5 = 5 
(0.0 -0.024748737341466 0.0247487373415923 ) // Vertex A6 = 6 
(0.0 -0.024748737341466 -0.0247487373415923 ) // Vertex A7 = 7 

//Plane B:
(3.5 0.0173241161390262 -0.0173241161391146 ) // Vertex B0 = 8 
(3.5 0.0173241161390262 0.0173241161391146 ) // Vertex B1 = 9 
(3.5 -0.0173241161390262 0.0173241161391146 ) // Vertex B2 = 10 
(3.5 -0.0173241161390262 -0.0173241161391146 ) // Vertex B3 = 11 
(3.5 0.024748737341466 -0.0247487373415923 ) // Vertex B4 = 12 
(3.5 0.024748737341466 0.0247487373415923 ) // Vertex B5 = 13 
(3.5 -0.024748737341466 0.0247487373415923 ) // Vertex B6 = 14 
(3.5 -0.024748737341466 -0.0247487373415923 ) // Vertex B7 = 15 

);

// Defining blocks:
blocks
(

    //Blocks between plane A and plane B:
    // block0 - positive x O-grid block
    hex (5 1 0 4 13 9 8 12 ) pipe
    (16 26 450)
    simpleGrading (3 1 1)
    // block1 - positive y O-grid block
    hex (6 2 1 5 14 10 9 13 ) pipe
    (16 26 450)
    simpleGrading (3 1 1)
    // block2 - negative x O-grid block
    hex (7 3 2 6 15 11 10 14 ) pipe
    (16 26 450)
    simpleGrading (3 1 1)
    // block3 - negative y O-grid block
    hex (4 0 3 7 12 8 11 15 ) pipe
    (16 26 450)
    simpleGrading (3 1 1)
    // block4 - central O-grid block
    hex (0 1 2 3 8 9 10 11 ) pipe
    (26 26 450)
    simpleGrading (1 1 1)
);

edges
(
    //Plane A:
    arc 0 1 (0.0 0.0196 0 )
    arc 1 2 (0.0 0 0.0196 )
    arc 2 3 (0.0 -0.0196 0 )
    arc 3 0 (0.0 0 -0.0196 )
    arc 4 5 (0.0 0.035 0 )
    arc 5 6 (0.0 0 0.035 )
    arc 6 7 (0.0 -0.035 0 )
    arc 7 4 (0.0 0 -0.035 )

    //Plane B:
    arc 8 9 (3.5 0.0196 0 )
    arc 9 10 (3.5 0 0.0196 )
    arc 10 11 (3.5 -0.0196 0 )
    arc 11 8 (3.5 0 -0.0196 )
    arc 12 13 (3.5 0.035 0 )
    arc 13 14 (3.5 0 0.035 )
    arc 14 15 (3.5 -0.035 0 )
    arc 15 12 (3.5 0 -0.035 )
);

// Defining patches:
patches
(
    patch INLET
    (
       (1 5 4 0)
       (2 6 5 1)
       (3 7 6 2)
       (0 4 7 3)
       (3 2 1 0)
      
    )

    patch OUTLET
    (
        (8 12 13 9)
       (9 13 14 10)
       (10 14 15 11)
       (11 15 12 8)
       (8 9 10 11)
    )

    wall PIPE_WALL
    (
       (4 5 13 12)
       (5 6 14 13)
       (6 7 15 14)
       (7 4 12 15)
    )
);

mergePatchPairs 
(
);

